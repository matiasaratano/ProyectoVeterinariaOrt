@model Clientes<h2 style="color:coral; text-align:center">AGREGAR CLIENTES</h2><form asp-action="CrearClientes">    <div class="form-group">        <label for="nombre" class="control-label">NOMBRE</label>        <input asp-for="Nombre" type="text" id="nombre"class="form-control" />    </div>     <div class="form-group">        <label for="DNI" class="control-label">DNI</label>        <input asp-for="DNI" type="text" id="DNI"class="form-control" />    </div>     <div class="form-group">        <label for="Apellido" class="control-label">Apellido</label>        <input asp-for="Apellido" type="text" id="Apellido"class="form-control" />    </div>     <div class="form-group">        <label for="Email" class="control-label">Email</label>        <input asp-for="Email" type="text" id="Email"class="form-control" />    </div>        <div class="form-group">        <label for="Telefono" class="control-label">Telefono</label>        <input asp-for="Telefono" type="text" id="Telefono"class="form-control" />    </div>    <div class="form-group">        <button type="submit" style="margin-top:10px; max-width:30%" class="btn-success form-control">Guardar</button>    </div>   </form>
